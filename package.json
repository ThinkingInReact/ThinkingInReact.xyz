  "scripts": {
    "start": "node server.js",
    "test:client": "karma start",
    "test:server": "mocha \"test/server/**/*.spec.js\"",
    "start:dev": "npm run webpack && nf start",
    "webpack:server": "webpack --config webpack.server.config.js",
    "webpack:client": "webpack --config webpack.client.config.js",
    "webpack:serverw": "webpack --config webpack.server.config.js --w",
    "webpack:clientw": "webpack --config webpack.client.config.js --w",
    "webpack": "npm run webpack:server && npm run webpack:client",
    "dist": "NODE_ENV=production npm run webpack:client",
    "deploy:gcloud": "gcloud preview app deploy --project thinking-in-react --stop-previous-version",
    "deploy:modulus": "npm run dist && npm run webpack:server && modulus deploy"
  },
